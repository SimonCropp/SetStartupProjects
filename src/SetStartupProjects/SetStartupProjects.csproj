<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net6.0</TargetFrameworks>
    <Authors>Simon Cropp</Authors>
    <Description>Hacks a Visual Studio .suo file to set multiple startup projects.</Description>
    <PackageTags>SUO, Visual Studio, Startup</PackageTags>
    <NoWarn>CS1591</NoWarn>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="ProjectDefaults" Version="1.0.101" PrivateAssets="all" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.1" PrivateAssets="All" Condition="$(Configuration) == 'Release'" />
    <EmbeddedResource Include="*.suotemplate" />
    <PackageReference Include="OpenMcdf" Version="2.3.0" />
    <PackageReference Include="Polyfill" Version="1.32.1" PrivateAssets="all" />
  </ItemGroup>
</Project>
